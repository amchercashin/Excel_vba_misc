Dim array() As Variant

If (Not Not array()) <> 0 Then
    ' Array has been initialized
Else
    ' Array has NOT been initialized
End If



'Consider using isEmpty on variant variable containing array:
Dim array As Variant

'set array = ActiveWorkSheet.Range("A1:A2")

If IsEmpty(array) then
    'array is not set
Else
    'array is set
End if
